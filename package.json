{
  "name": "@stokedconsulting/s3tube",
  "version": "1.0.0",
  "description": "S3 Tube is a conversion of YouTube Clone turned into a mono repo implementation with cenv to easily deploy to aws using an S3 bucket",
  "scripts": {
    "start:frontend": "pnpm -F @stokedconsulting/s3tube-spa start",
    "start:backend": "pnpm -F @stokedconsulting/s3tube-api start",
    "start": "./node_modules/.bin/concurrently --kill-others 'pnpm start:frontend' 'pnpm start:backend'"
  },
  "keywords": [],
  "author": "Brian Stoker",
  "license": "MIT",
  "devDependencies": {
    "@stoked-cenv/lib": "^2.0.0-a.99",
    "@types/node": "18.16.1",
    "@typescript-eslint/parser": "^5.62.0",
    "concurrently": "^8.2.0",
    "prettier": "^2.8.8",
    "typescript": "4.9.5"
  },
  "cli": {
    "packageManager": "pnpm"
  },
  "dependencies": {
    "cloudinary": "^1.40.0"
  }
}
        "@stoked-cenv/ui",
        "@stoked-cenv/lib",
        "@stoked-cenv/cli",
        "@stoked-cenv/cli-select",
        "@stoked-cenv/cdk"
      ]
    },
    "brianstoker": {
      "packages": [
        "brianstoker-api",
        "brianstoker-spa",
        "@stoked-cenv/cdk+cert@brianstoker"

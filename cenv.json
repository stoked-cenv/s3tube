    "default": {
      "packages": [
        "@stokedconsulting/common-spa",
        "brianstoker-api",
        "brianstoker-spa",
        "@stoked-cenv/cdk+cert@brianstoker",
        "stokedconsulting-spa",
        "@stoked-cenv/cdk+cert@stokedconsulting",
        "@stoked-cenv/cdk+network"
      ]
